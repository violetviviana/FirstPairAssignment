// Add import statements here.
import java.io.*;
import java.util.*;

public class WordProcessor{
	public static void main(String[] args) throws FileNotFoundException {
		Scanner console = new Scanner (System.in);
		Scanner input = new Scanner(new File(".txt"));
	}
	
    public static void consoleOutput (boolean passedTest) {
        System.out.print ("Enter name of file to be processed, including the .txt extension: ");
        if (passedTest){
            System.out.print ("The maximum output file line length is CORRECT.");
        }
        else {
            System.out.print ("The maximum output file line length is INCORRECT!!!");
        }
    }
	
	
}
